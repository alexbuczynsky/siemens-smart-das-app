{
  "name": "breaker-config-web-client",
  "author": {
    "name": "Siemens"
  },
  "version": "2.1.0-beta.4",
  "private": true,
  "dependencies": {
    "@smartgear/edison": "^0.8.1",
    "@smartgear/fonts": "^0.2.1",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "axios": "^0.19.0",
    "axios-retry": "^3.1.2",
    "express": "^4.17.1",
    "notistack": "^0.9.9",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "3.4.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-react-hook": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "storm-react-diagrams": "^5.2.1",
    "typescript": "3.8.3",
    "uuid": "^7.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:electron": "electron ./build/electron.js",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "build:electron:mac": "yarn build && electron-builder -m",
    "build:electron:linux": "yarn build && electron-builder -l",
    "build:electron:windows": "yarn build && electron-builder -w"
  },
  "main": "public/electron.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-redux": "^7.1.0",
    "@types/redux-logger": "^3.0.7",
    "@types/uuid": "^7.0.0",
    "copyfiles": "^2.1.0",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "build": {
    "appId": "com.siemens.si.lp.das-controller-setup",
    "productName": "Smart DAS Controller Configurator",
    "copyright": "Copyright Â© 2019 Siemens Inc.",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {},
    "linux": {},
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "../assets",
      "output": "../dist"
    },
    "extraResources": {
      "from": "../server/bin/dist/",
      "to": "./server/bin/dist/",
      "filter": [
        "**/*"
      ]
    }
  }
}
